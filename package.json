{
  "name": "ipinfo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "format": "prettier --write ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format && git add .",
      "pre-push": "yarn lint && yarn test && yarn build"
    }
  },
  "engines": {
    "yarn": ">= 1.22.10",
    "node": ">=14.17.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/react": "17.0.15",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "autoprefixer": "^10.3.1",
    "eslint": "^7.32.0",
    "eslint-config-next": "11.0.1",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "tailwindcss": "^2.2.7",
    "typescript": "4.3.5"
  }
}
